{
  "name": "gatsby-scaffolder",
  "bin": {
    "gtsf": "./src/index.js"
  },
  "version": "1.0.8",
  "description": "A command line tool that helps you scaffold various parts that you might need when developing for the Gatsby framework",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watchAll",
    "premake-badge": "$(npm bin)/jest --coverage",
    "make-badge": "$(npm bin)/coverage-badges"
  },
  "jest": {
    "coverageReporters": [
      "text",
      "lcov",
      "json-summary"
    ]
  },
  "keywords": [
    "gatsby",
    "command line",
    "scaffold",
    "react",
    "javascript",
    "cli"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/softchris/gatsby-cli.git"
  },
  "homepage": "https://github.com/softchris/gatsby-cli",
  "bugs": {
    "url": "https://github.com/softchris/gatsby-cli/issues"
  },
  "author": "Chris Noring (https://softchris.github.io)",
  "contributors": [
    {
      "name": "chris noring",
      "url": "https://softchris.github.io"
    }
  ],
  "license": "MIT",
  "dependencies": {
    "chalk": "^4.0.0",
    "commander": "^5.1.0",
    "figlet": "^1.4.0",
    "fs-extra": "^9.0.0"
  },
  "devDependencies": {
    "coverage-badges": "^1.0.4",
    "jest": "^26.0.1"
  }
}
